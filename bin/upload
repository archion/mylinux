#!/bin/bash
ip=$(who |grep YZD |grep -E "([0-9]{1,3}\.){3}[0-9]{1,3}" | awk -F'[()]' '{if ( NR ==1 )print $2}')
rsync -avz --delete archion@$ip:/home/archion/Projects/FortranProgram/ --exclude=".*" --exclude="*.out" --exclude="*.mod" --exclude="*.a" --exclude=data --exclude=makefile /home/YZD/Projects/FortranProgram
