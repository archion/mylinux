#!/bin/bash
ip=$(who |grep YZD |grep -oE "([0-9]{1,3}\.){3}[0-9]{1,3}")
ssh YZD@192.168.0.206 -p 2222 'rsync -avz /home/YZD/Projects/FortranProgram/data/ archion@$(who |grep YZD |grep -oE "([0-9]{1,3}\.){3}[0-9]{1,3}"|head -n 1):/home/archion/Projects/FortranProgram/data'
ssh YZD@192.168.0.206 -p 2222 'ssh -X archion@$(who |grep YZD |grep -oE "([0-9]{1,3}\.){3}[0-9]{1,3}") "DISPLAY=:0 notify-send \"Program complete!\" \"Run complete!\" --icon=dialog-information"'

